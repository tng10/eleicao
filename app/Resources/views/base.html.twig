{#{% extends 'XsolveBootstrapCrudBundle::examplelayout.html.twig' %}#}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}Welcome!{% endblock %}</title>

    {% block stylesheets_acme %}
        <link rel="stylesheet" href="{{ asset('bundles/acmedemo/css/demo.css') }}"/>
    {% endblock %}

    {% block stylesheets %}
        {% stylesheets
        'css/bootstrap.css'
        'bundles/xsolvebootstrapcrud/css/examplelayout.css' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}
    {% endblock %}

    {% block javascriptstop %}
        {% javascripts
        'js/jquery.js'
        'js/bootstrap.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}

    <link rel="icon" sizes="16x16" href="{{ asset('favicon.ico') }}"/>

</head>
{% block bootstrap_navbar %}
<!-- Navbar
================================================== -->
<div class="navbar navbar-inverse navbar-fixed-top">
    <div class="navbar-inner">
        <div class="container container-fluid">
            <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="brand"
               href="{% block bootstrap_title_link %}{% endblock %}">{% block bootstrap_title %}Bootstrap{% endblock %}
            </a>

            <div class="nav-collapse collapse row-fluid">
                {% block bootstrap_menu %}
                    {{ knp_menu_render('EleicaoAdmBundle:Builder:mainMenu') }}
                {% endblock %}
            </div>
        </div>
    </div>
</div>
{% endblock %}
{% block bootstrap_subhead %}
    <!-- Subhead
    ================================================== -->
    <!-- <header class="jumbotron subhead" id="overview">
        <div class="container container-fuild">
            <h1>Getting started</h1>

            <p class="lead">Overview of the project and how to get started with a simple template.</p>

        </div>
    </header> -->
{% endblock %}

<div class="container container-fluid">

    {% for flashKey, flashMessage in app.session.flashbag.get('notice') %}
        <div class="flash-notice img-rounded btn btn-info disabled btn-block" style="margin: 10px 0px;">
            {{ flashMessage }}
        </div>
    {% endfor %}

    <!-- Docs nav
    ================================================== -->
    <div class="row row-fluid">
        {% block mainbody %}
            <div class="span12 sf-reset">
                <section>
                    {% block body %}
                        <section id="contents">
                            <div class="page-header">
                                <h1>1. What's included</h1>
                            </div>
                            <p class="lead">Bootstrap comes equipped with HTML, CSS, and JS for all sorts of things, but
                                they can be summarized with a handful of categories visible at the top of the <a
                                        href="http://getbootstrap.com">Bootstrap documentation</a>.</p>
                        </section>
                    {% endblock %}
                </section>
            </div>
        {% endblock %}
    </div>

</div>


<!-- Footer
================================================== -->
{% block bootstrap_footer %}
    <footer class="footer">
        <div class="container">
            <p>&copy; 2014 - Sistema de Eleição</p>
        </div>
    </footer>
{% endblock %}

{% block javascripts %}{% endblock %}
</body>
</html>
